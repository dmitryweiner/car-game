{
  "name": "car-game",
  "version": "1.1.0",
  "description": "2D Online car simulator with AI.",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "node --experimental-modules ./src/scripts/truncate.mjs && node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --config webpack/webpack.config.dev.js",
    "train": "node --experimental-modules ./src/scripts/training.mjs",
    "truncate": "node --experimental-modules ./src/scripts/truncate.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "es6",
    "javascript",
    "neataptic",
    "neural"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmitryweiner/car-game/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.1",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "core-js": "^3.5.0",
    "neataptic": "^1.4.7"
  }
}
